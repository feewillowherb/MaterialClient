<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:FdSoft.Material.WpfClient"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:aforge="clr-namespace:AForge.Controls;assembly=AForge.Controls"
    xmlns:wfi="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
    xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
    x:Class="FdSoft.Material.WpfClient.MainWindow"
    mc:Ignorable="d" HorizontalContentAlignment="Stretch" Width="1366" Height="768"
    WindowStartupLocation="CenterScreen" Icon="/Resources/fd-ico.ico"
    Closing="Window_Closing" ResizeMode="NoResize" Loaded="Window_Loaded" ShowInTaskbar="True" WindowStyle="None"
    Title="凡东智能物料验收系统">

    <Window.Resources>
        <local:TimeFormatConverter x:Key="TimeFormatConverter" />
        <WindowChrome x:Key="WindowChromeKey">
            <WindowChrome.ResizeBorderThickness>
                <Thickness>0</Thickness>
            </WindowChrome.ResizeBorderThickness>
        </WindowChrome>
        <Style x:Key="Province" TargetType="{x:Type ComboBox}">
            <Setter Property="Width" Value="120"></Setter>
            <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ComboBox}">
                        <Border BorderBrush="Transparent" BorderThickness="1" CornerRadius="15,15,15,15"
                                Background="Transparent">
                            <Grid>
                                <!--下拉箭头-->
                                <ToggleButton ClickMode="Press" Focusable="False"
                                              IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                              Margin="2" MinWidth="0" MinHeight="0" Width="Auto">
                                    <ToggleButton.Style>
                                        <Style TargetType="{x:Type ToggleButton}">
                                            <Setter Property="MinWidth" Value="0" />
                                            <Setter Property="MinHeight" Value="0" />
                                            <Setter Property="Width" Value="Auto" />
                                            <Setter Property="Height" Value="Auto" />
                                            <Setter Property="Background" Value="Transparent" />
                                            <Setter Property="BorderBrush" Value="#00000000" />
                                            <Setter Property="Foreground" Value="White" />
                                            <Setter Property="BorderThickness" Value="1" />
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                                                        <DockPanel Background="{TemplateBinding Background}"
                                                                   LastChildFill="False" SnapsToDevicePixels="True">
                                                            <Border x:Name="Border"
                                                                    BorderBrush="{TemplateBinding BorderBrush}"
                                                                    BorderThickness="{TemplateBinding BorderThickness}"
                                                                    DockPanel.Dock="Right">
                                                                <!--<Path Data="M0,0L3.5,4 7,0z" Fill="{TemplateBinding Foreground}" HorizontalAlignment="Center" VerticalAlignment="Center"/>-->
                                                                <!--重新绘制下拉箭头，旋转90度-->
                                                                <Image Source="/Resources/down.png" Width="15"
                                                                       Height="10">
                                                                </Image>
                                                            </Border>
                                                        </DockPanel>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="IsChecked" Value="True"></Trigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                            <Style.Triggers>
                                                <Trigger Property="IsEnabled" Value="False">
                                                    <Setter Property="Foreground"
                                                            Value="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" />
                                                </Trigger>
                                            </Style.Triggers>
                                        </Style>
                                    </ToggleButton.Style>
                                </ToggleButton>
                                <!--项内容-->
                                <ContentPresenter IsHitTestVisible="False" Content="{TemplateBinding SelectionBoxItem}"
                                                  ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                                                  ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                                                  VerticalAlignment="Center" Margin="3" HorizontalAlignment="Center" />
                                <TextBox x:Name="PART_EditableTextBox" HorizontalAlignment="Stretch" Focusable="True"
                                         TextAlignment="Center" Visibility="Collapsed" IsReadOnly="False" />
                                <!--下拉显示面板HorizontalOffset:设置下拉面板的相对位置-->
                                <Popup HorizontalOffset="-1" Width="{TemplateBinding ActualWidth}"
                                       IsOpen="{TemplateBinding IsDropDownOpen}" Focusable="False"
                                       PopupAnimation="Slide">
                                    <Grid SnapsToDevicePixels="True" HorizontalAlignment="Stretch">
                                        <Border BorderThickness="1" BorderBrush="#2552B9" HorizontalAlignment="Stretch">
                                            <Border.Background>
                                                <SolidColorBrush Color="White" />
                                            </Border.Background>
                                        </Border>
                                        <ScrollViewer SnapsToDevicePixels="True" HorizontalAlignment="Stretch">
                                            <StackPanel IsItemsHost="True"
                                                        KeyboardNavigation.DirectionalNavigation="Contained"
                                                        HorizontalAlignment="Stretch" />
                                        </ScrollViewer>
                                    </Grid>
                                </Popup>
                            </Grid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="{x:Type TabItem}">
            <Setter Property="FontSize" Value="16" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TabItem}">
                        <Grid>
                            <Border Name="Border" Background="#FCFCFE" BorderBrush="#E4E4E4" BorderThickness="1"
                                    Width="126" Height="50">
                                <ContentPresenter Name="Content" VerticalAlignment="Center"
                                                  HorizontalAlignment="Center" ContentSource="Header"
                                                  RecognizesAccessKey="True" />
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="Border" Property="Background" Value="#4A85F9" />
                                <Setter TargetName="Border" Property="BorderThickness" Value="1" />
                                <Setter Property="Foreground" Value="#FFFFFF" />
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="Border" Property="Background" Value="Silver" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="Black" />
                                <Setter Property="Foreground" Value="DarkGray" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="EnabledBtn" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Background" Value="LightYellow"></Setter>
                                <Setter Property="BorderBrush" Value="LightYellow"></Setter>
                                <Setter Property="BorderThickness" Value="1"></Setter>
                                <Setter Property="Foreground" Value="White"></Setter>
                                <Setter Property="FontSize" Value="20"></Setter>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ContentAdd" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="Transparent"></Setter>
            <Setter Property="Foreground" Value="White"></Setter>
            <Setter Property="UseLayoutRounding" Value="False"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <DockPanel Width="40" Height="40" VerticalAlignment="Center" HorizontalAlignment="Center">
                            <Border BorderBrush="#2AC2BD" BorderThickness="2" Background="#2AC2BD" Cursor="Hand"
                                    ToolTip="新增材料">
                                <materialDesign:PackIcon Kind="Add" Height="20" Width="20" VerticalAlignment="Center"
                                                         HorizontalAlignment="Center" />
                            </Border>
                        </DockPanel>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ContentDel" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="Transparent"></Setter>
            <Setter Property="Foreground" Value="Red"></Setter>
            <Setter Property="UseLayoutRounding" Value="False"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <DockPanel Width="110" Height="40" VerticalAlignment="Center" HorizontalAlignment="Center">
                            <Border BorderBrush="#E6E6E6" BorderThickness="1" Background="Transparent" Cursor="Hand">
                                <materialDesign:PackIcon Kind="DeleteOutline" Height="20" Width="20"
                                                         VerticalAlignment="Center" HorizontalAlignment="Center" />
                            </Border>
                        </DockPanel>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ContentItem" TargetType="{x:Type Label}">
            <Setter Property="Width" Value="120"></Setter>
            <Setter Property="Height" Value="40"></Setter>
            <!--<Setter Property="Margin" Value="5,0,5,0"></Setter>-->
            <Setter Property="FontSize" Value="13"></Setter>
            <Setter Property="Padding" Value="0,10,0,5"></Setter>
            <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
            <Setter Property="HorizontalAlignment" Value="Center"></Setter>
            <Setter Property="BorderBrush" Value="#EDEDED"></Setter>
            <Setter Property="BorderThickness" Value="1"></Setter>
            <Setter Property="HorizontalAlignment" Value="Left"></Setter>
        </Style>
        <Style x:Key="EnabelBtn" TargetType="{x:Type Button}">
            <Setter Property="VerticalAlignment" Value="Top"></Setter>
            <Setter Property="HorizontalAlignment" Value="Left"></Setter>
            <Setter Property="Foreground" Value="White"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid>
                            <Border Name="enabel" BorderBrush="#2ac2bd" BorderThickness="1" CornerRadius="10">
                                <Border.Background>#2ac2bd</Border.Background>
                                <ContentPresenter Content="{TemplateBinding ContentControl.Content}"
                                                  HorizontalAlignment="Center" VerticalAlignment="Center">
                                </ContentPresenter>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="enabel" Property="Opacity" Value="0.6" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="defaultTextBox" TargetType="{x:Type TextBox}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TextBox}">
                        <TextBox BorderBrush="#E6E6E6">
                            <TextBox.Resources>
                                <VisualBrush x:Key="HintText" TileMode="None" Opacity="0.5" Stretch="None"
                                             AlignmentX="Center">
                                    <VisualBrush.Visual>
                                        <TextBlock FontSize="13" Text="请输入" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center">
                                        </TextBlock>
                                    </VisualBrush.Visual>
                                </VisualBrush>
                            </TextBox.Resources>
                            <TextBox.Style>
                                <Style TargetType="TextBox">
                                    <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
                                    <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
                                    <Style.Triggers>
                                        <Trigger Property="Text" Value="{x:Null}">
                                            <Setter Property="Background" Value="{StaticResource HintText}" />
                                        </Trigger>
                                        <Trigger Property="Text" Value="">
                                            <Setter Property="Background" Value="{StaticResource HintText}" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </TextBox.Style>
                        </TextBox>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>

        </Style>
    </Window.Resources>

    <Viewbox>
        <materialDesign:DialogHost x:Name="dialogSetting">
            <materialDesign:DialogHost.DialogContent>
                <UniformGrid Columns="1" Width="500" Height="300" Background="White" VerticalAlignment="Center"
                             HorizontalAlignment="Center">
                    <TextBlock Height="50" FontSize="36" Foreground="#2196F3" HorizontalAlignment="Center"
                               VerticalAlignment="Center">
                        系统已锁定
                    </TextBlock>
                    <PasswordBox x:Name="LockPwd" Width="300" Height="55" FontSize="30" BorderBrush="LightBlue"
                                 BorderThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                    </PasswordBox>
                    <Button Width="250" Height="55" FontSize="24" Click="dialog_Click">输入密码解锁</Button>
                </UniformGrid>
            </materialDesign:DialogHost.DialogContent>
            <Grid>
                <DockPanel Margin="0" x:Name="dockPanel" Width="1920" Height="1080" Background="#F5F6FA">
                    <!--Header Menu 50-->
                    <DockPanel Height="50" MouseMove="TitleBar_MouseMove" MouseDown="TitleBar_MouseDown"
                               Background="#2D66E7" DockPanel.Dock="Top" VerticalAlignment="Top" x:Name="headerMenu">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="/Resources/Indexlogo.png" Width="210" HorizontalAlignment="Center"
                                   Margin="25,0,80,0" Cursor="Hand" PreviewMouseUp="Btn_New_Click" />
                        </StackPanel>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="400"></ColumnDefinition>
                                <ColumnDefinition Width="150"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Menu Grid.Row="0" Grid.Column="0" Height="49" Background="#FF2D66E7" Foreground="White"
                                  FontSize="16" DockPanel.Dock="Top" HorizontalAlignment="Left" OpacityMask="White">
                                <MenuItem Header="数据管理" Foreground="White" BorderBrush="Black">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/Data.png" Margin="0,0,-25,0"></Image>
                                    </MenuItem.Icon>
                                    <MenuItem Click="MIMaterialMrg_Click" Background="#FF2D66E7" Focusable="True"
                                              Foreground="White" Margin="0" Padding="0,5" VerticalAlignment="Center"
                                              FontSize="16" Height="45">
                                        <MenuItem.Header>
                                            <TextBlock Width="100" TextAlignment="Center">材料管理</TextBlock>
                                        </MenuItem.Header>
                                        <MenuItem.Icon>
                                            <Image Source="/Resources/Material_checked.png" Margin="0,0,-50,0"></Image>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                    <MenuItem Click="MIProviderMrg_Click" Background="#FF2D66E7" Focusable="True"
                                              Foreground="White" Margin="0" Padding="0,5" VerticalAlignment="Center"
                                              FontSize="16" Height="45">
                                        <MenuItem.Header>
                                            <TextBlock Width="100" TextAlignment="Center">供应管理</TextBlock>
                                        </MenuItem.Header>
                                        <MenuItem.Icon>
                                            <Image Source="/Resources/supplier_checked.png" Margin="0,0,-50,0"></Image>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                    <MenuItem Click="MIOrderMrg_Click" Background="#FF2D66E7" Focusable="True"
                                              Foreground="White" Margin="0" Padding="0,5" VerticalAlignment="Center"
                                              FontSize="16" Height="45">
                                        <MenuItem.Header>
                                            <TextBlock Width="100" TextAlignment="Center">台账管理</TextBlock>
                                        </MenuItem.Header>
                                        <MenuItem.Icon>
                                            <Image Source="/Resources/Ledger_checked.png" Margin="0,0,-50,0"></Image>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                </MenuItem>

                                <MenuItem Header="系统设置" Click="MIBaseConfig_Click" Focusable="True">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/SystemSetting.png" Margin="0,0,-25,0"></Image>
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Header="项目信息" Click="ProjectInfo_Click" Focusable="True">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/Version.png" Margin="0,0,-25,0"></Image>
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Header="数据同步" Click="MINUpdateOnline" x:Name="SyncDataBtn" Focusable="True">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/DataUpdate.png" Margin="0,0,-20,0"></Image>
                                    </MenuItem.Icon>
                                </MenuItem>
                            </Menu>
                            <Menu Grid.Row="0" Grid.Column="1" Height="49" Background="#FF2D66E7" Foreground="#B8CBFE"
                                  FontSize="16" DockPanel.Dock="Top" HorizontalAlignment="Right" OpacityMask="#B8CBFE"
                                  VerticalAlignment="Top" Visibility="Collapsed">
                                <MenuItem Header="扫码模式" Foreground="White" BorderBrush="Black" Click="QrCode_Click">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/User.png" Margin="0,0,-25,0"></Image>
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Header="无人值守模式" Foreground="White" BorderBrush="Black"
                                          Click="Unattended_Click">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/User.png" Margin="0,0,-25,0"></Image>
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Header="--" x:Name="mi_Account" Background="#709AFC" Foreground="White"
                                          BorderBrush="Black" HorizontalAlignment="Right" Height="35" FontSize="16"
                                          Margin="0,7.5,0,7.5">
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/User.png" Margin="0,0,-25,0"></Image>
                                    </MenuItem.Icon>
                                    <MenuItem Click="MenuItem_Click" Visibility="Collapsed" Background="#FF2D66E7"
                                              Foreground="White" Margin="0" Padding="0,5" VerticalAlignment="Center"
                                              FontSize="16" Height="45">
                                        <MenuItem.Header>
                                            <TextBlock Width="120" TextAlignment="Center">密码修改</TextBlock>
                                        </MenuItem.Header>
                                        <MenuItem.Icon>
                                            <Image Source="/Resources/SystemSetting.png" Margin="0,0,-50,0"></Image>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                    <MenuItem Click="OutLogin_Click" Visibility="Collapsed" Background="#FF2D66E7"
                                              Foreground="White" Margin="0" Padding="0,5" VerticalAlignment="Center"
                                              FontSize="16" Height="45">
                                        <MenuItem.Header>
                                            <TextBlock Width="120" TextAlignment="Center">退出登录</TextBlock>
                                        </MenuItem.Header>
                                        <MenuItem.Icon>
                                            <Image Source="/Resources/LoginOut.png" Margin="0,0,-50,0"></Image>
                                        </MenuItem.Icon>
                                    </MenuItem>
                                </MenuItem>
                            </Menu>
                            <DockPanel Grid.Row="0" Grid.Column="2">
                                <Button Click="btn_close_Click" Height="35" Background="{x:Null}"
                                        BorderBrush="Transparent" HorizontalContentAlignment="Center"
                                        HorizontalAlignment="Right" VerticalContentAlignment="Stretch" FontSize="16"
                                        Margin="0,0,10,0" DockPanel.Dock="Right">
                                    <TextBlock FontSize="30" Foreground="White" HorizontalAlignment="Left"
                                               VerticalAlignment="Center">
                                        <Run Text="×" />
                                    </TextBlock>
                                </Button>
                                <Button Click="btn_min_Click" Height="35" Background="{x:Null}"
                                        BorderBrush="Transparent" HorizontalContentAlignment="Center"
                                        HorizontalAlignment="Right" VerticalContentAlignment="Stretch" FontSize="16"
                                        Margin="0,0,10,0" DockPanel.Dock="Right">
                                    <TextBlock FontSize="30" Foreground="White" HorizontalAlignment="Left"
                                               VerticalAlignment="Center">
                                        <Run Text="-" />
                                    </TextBlock>
                                </Button>
                            </DockPanel>
                        </Grid>
                    </DockPanel>
                    <!--Header Banner 190-->
                    <DockPanel Height="190" VerticalAlignment="Top" DockPanel.Dock="Top" Background="#4A85F9"
                               x:Name="headBanner" Visibility="Collapsed">
                        <UniformGrid Rows="1" DockPanel.Dock="Right">
                            <UniformGrid.Background>
                                <ImageBrush ImageSource="/Resources/Indexbanner.png"></ImageBrush>
                            </UniformGrid.Background>
                            <UniformGrid Rows="2" Columns="1" Width="600 " HorizontalAlignment="Left">
                                <Button Width="260" Height="50" Click="Btn_New_Click" Content="新增称重"
                                        Visibility="Hidden" HorizontalAlignment="Right" VerticalAlignment="Top"
                                        FontSize="22" FontWeight="Normal" BorderBrush="{x:Null}" Margin="0,25,0,0">
                                    <Button.Template>
                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <Border BorderBrush="#F5AA18" BorderThickness="1" CornerRadius="10">
                                                <Border.Background>#F5AA18</Border.Background>
                                                <ContentPresenter Content="{TemplateBinding ContentControl.Content}"
                                                                  HorizontalAlignment="Center"
                                                                  VerticalAlignment="Center">
                                                </ContentPresenter>
                                            </Border>
                                        </ControlTemplate>
                                    </Button.Template>
                                </Button>
                                <DockPanel HorizontalAlignment="Right" Width="260" Height="50" Margin="0,0,0,28">
                                    <Border Background="#FF3C6BC7" BorderBrush="#FF3C6BC7" BorderThickness="1"
                                            Width="260" CornerRadius="10">
                                        <DockPanel>
                                            <ComboBox x:Name="cb_TruckNoHead" SelectedValuePath="Content"
                                                      Style="{StaticResource Province}" Width="79" Height="47"
                                                      Foreground="White" FontSize="24"
                                                      HorizontalContentAlignment="Center" BorderThickness="0"
                                                      IsEnabled="False">
                                                <ComboBoxItem Content="京" Foreground="Black" />
                                                <ComboBoxItem Content="津" Foreground="Black" />
                                                <ComboBoxItem Content="沪" Foreground="Black" />
                                                <ComboBoxItem Content="渝" Foreground="Black" />
                                                <ComboBoxItem Content="冀" Foreground="Black" />
                                                <ComboBoxItem Content="豫" Foreground="Black" />
                                                <ComboBoxItem Content="云" Foreground="Black" />
                                                <ComboBoxItem Content="辽" Foreground="Black" />
                                                <ComboBoxItem Content="黑" Foreground="Black" />
                                                <ComboBoxItem Content="湘" Foreground="Black" />
                                                <ComboBoxItem Content="皖" Foreground="Black" />
                                                <ComboBoxItem Content="鲁" Foreground="Black" />
                                                <ComboBoxItem Content="新" Foreground="Black" />
                                                <ComboBoxItem Content="苏" Foreground="Black" />
                                                <ComboBoxItem Content="浙" Foreground="Black" IsSelected="True" />
                                                <ComboBoxItem Content="赣" Foreground="Black" />
                                                <ComboBoxItem Content="鄂" Foreground="Black" />
                                                <ComboBoxItem Content="桂" Foreground="Black" />
                                                <ComboBoxItem Content="甘" Foreground="Black" />
                                                <ComboBoxItem Content="晋" Foreground="Black" />
                                                <ComboBoxItem Content="蒙" Foreground="Black" />
                                                <ComboBoxItem Content="陕" Foreground="Black" />
                                                <ComboBoxItem Content="吉" Foreground="Black" />
                                                <ComboBoxItem Content="闽" Foreground="Black" />
                                                <ComboBoxItem Content="贵" Foreground="Black" />
                                                <ComboBoxItem Content="粤" Foreground="Black" />
                                                <ComboBoxItem Content="青" Foreground="Black" />
                                                <ComboBoxItem Content="藏" Foreground="Black" />
                                                <ComboBoxItem Content="川" Foreground="Black" />
                                                <ComboBoxItem Content="宁" Foreground="Black" />
                                                <ComboBoxItem Content="琼" Foreground="Black" />
                                                <ComboBoxItem Content="使" Foreground="Black" />
                                                <ComboBoxItem Content="领" Foreground="Black" />
                                            </ComboBox>
                                            <TextBox Width="152" Height="47" x:Name="txt_TruckNoBody"
                                                     Foreground="White" FontSize="24"
                                                     HorizontalContentAlignment="Center" BorderThickness="0"
                                                     VerticalAlignment="Center" Text=""
                                                     VerticalContentAlignment="Center"
                                                     TextChanged="txt_TruckNoBody_TextChanged"
                                                     IsReadOnly="True" />
                                        </DockPanel>
                                    </Border>
                                </DockPanel>
                            </UniformGrid>
                            <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="535" Height="115">
                                <Border BorderBrush="#4175DB" BorderThickness="1" Background="#4175DB"
                                        CornerRadius="15" HorizontalAlignment="Right">
                                    <DockPanel>
                                        <UniformGrid Rows="2" Columns="1" Width="110" HorizontalAlignment="Left"
                                                     Margin="10">
                                            <RadioButton x:Name="rb_Material_Revice" IsChecked="True" Width="100"
                                                         Height="40" GroupName="button1" HorizontalAlignment="Center"
                                                         VerticalAlignment="Center">
                                                <RadioButton.Template>
                                                    <ControlTemplate TargetType="RadioButton">
                                                        <Border Name="bd" BorderThickness="1" CornerRadius="4"
                                                                BorderBrush="Transparent" Background="Transparent">
                                                            <TextBlock Name="bdText" Text="收料" Foreground="#FFFFFF"
                                                                       FontSize="20" TextAlignment="Center"
                                                                       Padding="0,7,0,0">
                                                            </TextBlock>
                                                        </Border>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="IsChecked" Value="True">
                                                                <Setter TargetName="bd" Property="Background"
                                                                        Value="#FFFFFF" />
                                                                <Setter TargetName="bd" Property="BorderBrush"
                                                                        Value="#FFFFFF" />
                                                                <Setter TargetName="bdText" Property="Foreground"
                                                                        Value="#3B7DF8" />
                                                            </Trigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </RadioButton.Template>
                                            </RadioButton>
                                            <RadioButton x:Name="rb_Material_Send" Width="100" Height="40"
                                                         GroupName="button1" HorizontalAlignment="Center"
                                                         VerticalAlignment="Center">
                                                <RadioButton.Template>
                                                    <ControlTemplate TargetType="RadioButton">
                                                        <Border Name="bd" BorderThickness="1" CornerRadius="4"
                                                                BorderBrush="Transparent" Background="Transparent">
                                                            <TextBlock Name="bdText" Text="发料" Foreground="#FFFFFF"
                                                                       FontSize="20" TextAlignment="Center"
                                                                       Padding="0,7,0,0">
                                                            </TextBlock>
                                                        </Border>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="IsChecked" Value="True">
                                                                <Setter TargetName="bd" Property="Background"
                                                                        Value="#FFFFFF" />
