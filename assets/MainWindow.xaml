                                </DataGridTextColumn>
                                <DataGridTextColumn Binding="{Binding Specifications}" Width="120" FontSize="18"
                                                    IsReadOnly="True" CanUserSort="False">
                                    <DataGridTextColumn.Header>
                                        <TextBlock FontSize="18">规格型号</TextBlock>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Binding="{Binding BasicUnit}" Width="100" FontSize="18"
                                                    IsReadOnly="True" CanUserSort="False">
                                    <DataGridTextColumn.Header>
                                        <TextBlock FontSize="18">单位</TextBlock>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                        <StackPanel Grid.Row="3" Orientation="Horizontal">
                            <Button x:Name="lbl_Goods_First" Content="首页" Height="25" Margin="2,0,0,0" FontSize="11"
                                    BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#DD1C71EC" Padding="10,4"
                                    LostFocus="TextBox_LostMouseCapture_Event" Click="Btn_GoodsPage_Click" />
                            <Button x:Name="lbl_Goods_Prev" Content="上页" Height="25" Margin="2,0,0,0" FontSize="11"
                                    BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#DD1C71EC" Padding="10,4"
                                    LostFocus="TextBox_LostMouseCapture_Event" Click="Btn_GoodsPage_Click" />
                            <Button x:Name="lbl_Goods_Next" Content="下页" Height="25" Margin="2,0,0,0" FontSize="11"
                                    BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#DD1C71EC" Padding="10,4"
                                    LostFocus="TextBox_LostMouseCapture_Event" Click="Btn_GoodsPage_Click" />
                            <Button x:Name="lbl_Goods_Last" Content="末页" Height="25" Margin="2,0,0,0" FontSize="11"
                                    BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#DD1C71EC" Padding="10,4"
                                    LostFocus="TextBox_LostMouseCapture_Event" Click="Btn_GoodsPage_Click" />
                            <Label x:Name="lbl_Goods_TotalCount" Content="当前页:1" Width="80" Height="25" FontSize="16" />
                            <Label x:Name="lbl_Goods_CurrCount" Content="共0条" Width="100" Height="25" FontSize="16" />
                        </StackPanel>
                    </Grid>
                </DockPanel>
                <materialDesign:Snackbar x:Name="SnackbarSeven" MessageQueue="{materialDesign:MessageQueue}"
                                         VerticalAlignment="Center" />
            </Grid>
        </materialDesign:DialogHost>
    </Viewbox>
</Window>